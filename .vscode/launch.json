{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Chrome",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}/frontend/src",
      "sourceMaps": true
    },
    {
      "name": "JsonRPC: Remote Attach",
      "type": "debugpy",
      "request": "attach",
      "pathMappings": [
        {
          "localRoot": "${workspaceFolder}",
          "remoteRoot": "/app"
        }
      ],
      "connect": {
        "port": 4678,
        "host": "localhost"
      },
      "justMyCode": true
    },
    {
      "name": "GenVM: Remote Attach",
      "type": "debugpy",
      "request": "attach",
      "pathMappings": [
        {
          "localRoot": "${workspaceFolder}",
          "remoteRoot": "/app"
        }
      ],
      "connect": {
        "port": 6678,
        "host": "localhost"
      },
      "justMyCode": true
    },
    {
      "name": "SQLAlchemy tests: Remote Attach",
      "type": "debugpy",
      "request": "attach",
      "pathMappings": [
        {
          "localRoot": "${workspaceFolder}/backend",
          "remoteRoot": "/app/backend"
        },
        {
          "localRoot": "${workspaceFolder}/tests/db-sqlalchemy",
          "remoteRoot": "/app"
        }
      ],
      "connect": {
        "port": 5678,
        "host": "localhost"
      },
      "justMyCode": true
    }
  ],
  "compounds": [
    {
      "name": "Debug All",
      "configurations": ["JsonRPC: Remote Attach", "GenVM: Remote Attach"]
    }
  ]
}
